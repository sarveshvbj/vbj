<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/cron_groups.xsd">
    <group id="update_vaibhav_stock_inventory">
        <schedule_generate_every>10</schedule_generate_every>
        <schedule_ahead_for>10</schedule_ahead_for>
        <schedule_lifetime>10</schedule_lifetime>
        <history_cleanup_every>10</history_cleanup_every>
        <history_success_lifetime>5</history_success_lifetime>
        <history_failure_lifetime>100</history_failure_lifetime>
        <use_separate_process>1</use_separate_process>
    </group>
</config>
