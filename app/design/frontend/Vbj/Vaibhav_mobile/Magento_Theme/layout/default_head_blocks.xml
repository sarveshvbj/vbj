<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0"/>

        <link rel="stylesheet" type="text/css" src="css/free-v4-font-face.min.css" />
        <link rel="stylesheet" type="text/css" src="css/free.min.css" />
        <link rel="stylesheet" type="text/css" src="css/v_owl.theme.default.min.css" />
        <link rel="stylesheet" type="text/css" src="css/v_owl.carousel.min.css" />
        <link rel="stylesheet" type="text/css" src="css/v_bootstrap.min.css" />
        <link rel="stylesheet" type="text/css" src="css/vbj_custom.css" />
        
        <remove src="fonts/opensans/light/opensans-300.woff2"/>
        <remove src="fonts/opensans/regular/opensans-400.woff2"/>
        <remove src="fonts/opensans/semibold/opensans-600.woff2"/>
        <remove src="fonts/opensans/bold/opensans-700.woff2"/>
        <remove src="fonts/Blank-Theme-Icons/Blank-Theme-Icons.woff2"/>
                
    </head>
    <body>
       <referenceContainer name="after.body.start">
            <block class="Magento\Framework\View\Element\Js\Components" name="head.components" as="components" template="Magento_Theme::js/components.phtml" before="-"/>
        </referenceContainer>
    </body>
</page>
