/*Filters*/
.filters{
    color: #FFF;
    display: flex;
    font-size: 12px;
    padding-left: 3rem;
    font-family: avenir;
    text-transform: uppercase;
    background-color: #6d2080;
    justify-content: space-between;
}
.filter-list {
    z-index: 3;
    width: 100%;
    padding: 1rem;
    cursor: pointer;
    position: relative;
    border-right: 1px solid #FFF;
}
.filter-list:before {
    top: 15px;
    content: '';
    width: 15px;
    right: 10px;
    height: 15px;
    display: block;
    position: absolute;
    border-radius: 50%;
    background-color: #000;
    border: 1px solid #000;

}
.filter-list:after {
    top: 50%;
    width: 5px;
    height: 5px;
    content: '';
    right: 15px;
    display: block;
    margin-top: -5px;
    position: absolute;
    border: 1px solid #FFF;
    transform: rotate(45deg);
    border-width: 0 2px 2px 0;
}
.filter-items{
    left: 0;
    right: 0;
    top: 100%;
    margin: 0;
    padding: 0;
    color: #000;
    display: none;
    position: absolute;
    background-color: #FFF;
    border: 1px solid #EEEEEE;
}
.filter-item-list {
    display: block;
    padding: 1rem;
    margin-bottom: 0px;
    border-bottom: 2px solid #EEEEEE;
}
.filter-list:hover:before{display: none;}
.filter-list:hover:after{border-width: 2px 0 0 2px;}
.filter-list:hover{background-color: #B96ECB;}
.filter-list:hover .filter-items{display: block;}
