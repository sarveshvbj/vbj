.tick_mark {
  display: block;
  position: relative;
  padding-left: 23px;
  margin-bottom: 12px;
  cursor: pointer;
  font-size: 12px;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  float: left;
}
.morefilter_popup {
    padding-top: 0;
}
.count_num {
    float: right;
    margin-right: 0.8em;
    border: 1px solid;
    font-size: 12px;
    padding: 1px 2px;
    border-radius: 3px;
}
/* Hide the browser's default checkbox */
.tick_mark input {
  position: absolute;
  opacity: 0;
  cursor: pointer;
  height: 0;
  width: 0;
  left: 0;
  z-index: 99;
}
/* Create a custom checkbox */
.checkmark {
    position: absolute;
    top: 0;
    left: 0;
    height: 15px;
    width: 15px;
    border-radius: 2px;
    border: 1px solid;
}

.checkmark:after {
  content: "";
  position: absolute;
  display: none;
}

/* Show the checkmark when checked */
.tick_mark input:checked ~ .checkmark:after {
  display: block;
}

/* Style the checkmark/indicator */
.tick_mark .checkmark:after {
    left: 4px;
    top: 1px;
    width: 6px;
    height: 9px;
    border: solid black;
    border-width: 0 3px 3px 0;
    -webkit-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    transform: rotate(45deg);
}
.morefilter_popup .col {
    border-right: 1px solid #ccc;
    padding-right: 5px;
    padding-bottom: 10px;
