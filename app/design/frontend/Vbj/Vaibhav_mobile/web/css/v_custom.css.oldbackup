/********** carousel ********** */
.carousel-inner img {
    width: 100%;
}
ul.carousel-indicators li {
    width: 10px;
    height: 10px;
    border-radius: 100%;
}
.dropdown-toggle::after {
    font-family: FontAwesome !important;
    content: '\f107' !important;
    border: none !important;
    vertical-align: middle !important;
}
.carousel {
    width: 100%;
}
.carousel-caption {
    background-color: rgba(0, 0, 0, 0.5);
    padding: 20px;
    border-radius: .5rem;
}

/******** slider ******/
.slide-box {
    display: flex;
    justify-content: space-between;
}

@media (min-width: 576px) and (max-width: 767.98px) {
    .slide-box img {
        -ms-flex: 0 0 50%;
        flex: 0 0 50%;
        max-width: 50%;
    }
}

@media (min-width: 768px) and (max-width: 991.98px) {
    .slide-box img {
        -ms-flex: 0 0 33.3333%;
        flex: 0 0 33.3333%;
        max-width: 33.3333%;
    }
}
.desktop-section {
    box-shadow: 0px 0px 14px -8px #000;
}

/******* header ******/
.header {
    background-color: #fff;
    z-index: 99;
}
.header .header-top img {
    width: 20px;
    vertical-align: sub;
}
.header .header-top {
    font-size: 13px;
}
.header a, .menu a {
    color: #6e2083;
    /*mani hided */
    /*font-size: 14px;*/
}
.menu a {
    font-weight: 600;
}
.menu .category-items {
    width: 75%;
}
.header-height {
    height: 148px;
}
@media only screen and (max-width: 1024px) {
    .menu .category-items {
        width: 100%;
    }
    .header-height {
        height: 125px;
    }
}
.header .nav-search .dropdown-toggle::after {
    display: none;
}
.header .search-dropdown {
    right: 0;
    background-color: #fff;
    z-index: 99;
    top: 105%;
}
.header .search-dropdown input {
    width: 500px;
    border: 1px solid #ccc;
    border-radius: 4px;
    height: 45px;
    background-color: #fff;
    padding: 0 10px;
}
.header .search-dropdown input::placeholder {
    color: #ccc;
    font-weight: 500;
}
.header .search-dropdown button {
    position: absolute;
    right: 0;
    height: 45px;
    background-color: #6e2083;
    color: #fff;
    font-weight: 500;
    border: 0;
    border-radius: 2px;
    padding: 0 14px;
    top:-1px;
}
.header .search-suggestion {
    box-shadow: -4px 4px 8px -4px #000;
}
.header .search-suggestion li {
    padding: 5px;
    font-size: 12px;
}
.header .search-suggestion li i {
    padding-right: 5px;
}
.header .search-suggestion > i {
    right: 15px;
    top: 15px;
}
.header .search-suggestion li a {
    font-size: 15px;
    color: #167ac6;
}

