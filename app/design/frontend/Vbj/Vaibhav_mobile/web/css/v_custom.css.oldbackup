    font-weight: 500;
    font-size: 16px;
}
.text-underline {
    position: relative;
}
.text-underline:after {
    content: '';
    width: 139px;
    left: -10%;
    width: 35%;
    background: #000;
    position: absolute;
    content: '';
    height: 1px;
    bottom: -5px;
}
.gradient-button {
    background-image: linear-gradient(to right, #e459e0, #9362f2);
    border: 0;
    border-radius: 3px;
    color: #fff;
    font-weight: 500;
}
.opacity-60 {
    opacity: 0.6;
}
.gradient-title {
    position: relative;
}
.gradient-title span {
    background-color: #fff;
    z-index: 1;
    position: relative;
    padding: 0 20px;
    font-size: 20px;
}
.gradient-title:before {
    content: '';
    height: 4px;
    width: 50%;
    right: 0;
    transform: translateY(-50%);
    top: 50%;
    position: absolute;
    background: transparent;
    background: -moz-linear-gradient(left, rgba(255, 255, 255, 0) 50%, rgba(0, 0, 0, 1) 100%);
    background: -webkit-gradient(left top, right top, color-stop(50%, rgba(255, 255, 255, 0)), color-stop(100%, rgba (0, 0, 0, 1)));
    background: -webkit-linear-gradient(left, rgba(255, 255, 255, 0) 50%, rgba(0, 0, 0, 1) 100%);
    background: -o-linear-gradient(left, rgba(255, 255, 255, 0) 50%, rgba(0, 0, 0, 1) 100%);
    background: -ms-linear-gradient(left, rgba(255, 255, 255, 0) 50%, rgba(0, 0, 0, 1) 100%);
    background: linear-gradient(to right, rgb(0, 0, 0) 50%, rgba(255, 255, 255, 0) 100%);
    border: none;
    border-radius: 50%;
}
.gradient-title:after {
    content: '';
    height: 4px;
    width: 50%;
    left: 0;
    transform: translateY(-50%);
    top: 50%;
    position: absolute;
    background: #000;
    background: -moz-linear-gradient(left, rgba(0, 0, 0, 1) 0%, rgba(255, 255, 255, 0) 58%);
    background: -webkit-gradient(left top, right top, color-stop(0%, rgba(0, 0, 0, 1)), color-stop(58%, rgba (255, 255, 255, 0)));
    background: -webkit-linear-gradient(left, rgba(0, 0, 0, 1) 0%, rgba(255, 255, 255, 0) 58%);
    background: -o-linear-gradient(left, rgba(0, 0, 0, 1) 0%, rgba(255, 255, 255, 0) 58%);
    background: -ms-linear-gradient(left, rgba(0, 0, 0, 1) 0%, rgba(255, 255, 255, 0) 58%);
    background: linear-gradient(to right, rgba(0, 0, 0, 0) 0%, rgb(0, 0, 0) 58%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#000000', endColorstr='#ffffff', GradientType=1);
    border: none;
    border-radius: 50%;
}
